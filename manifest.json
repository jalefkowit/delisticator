{
  "manifest_version": 3,

  "name": "BuzzFeed Delisticator",
  "description": "De-listicate BuzzFeed.",
  "version": "1.1",

  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },

  "content_scripts": [
  {
    "matches": ["https://www.buzzfeed.com/"],
    "js": ["jquery-2.0.3.js", "mutation_summary.js", "tree_mirror.js", "delisticator.js"],
    "css": ["delisticator.css"]
  }]
  
}
